# VulnZero Frontend Environment Variables

# API Base URL (Backend API)
VITE_API_BASE_URL=http://localhost:8000

# WebSocket Base URL (Backend WebSocket)
VITE_WS_BASE_URL=http://localhost:8000

# Environment (development, staging, production)
VITE_ENVIRONMENT=development

# Application Version
VITE_APP_VERSION=1.0.0

# Enable debug mode
VITE_DEBUG=true

# Enable verbose logging (logs all messages even in production)
# Useful for troubleshooting production issues
VITE_VERBOSE_LOGGING=false

# Feature flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_NOTIFICATIONS=true

# Error Tracking - Sentry DSN
# Get your DSN from https://sentry.io/settings/[your-org]/projects/[your-project]/keys/
# Leave empty to disable error tracking
VITE_SENTRY_DSN=
